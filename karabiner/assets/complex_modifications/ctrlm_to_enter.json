{
  "title": "ctrl-m_to_enter",
  "rules": [
    {
      "description": "Ctrl-M を Enter に変換",
      "manipulators": [
        { "type": "basic",
          "from": { "key_code": "m",
            "modifiers": { "mandatory": [ "control" ] }
                  },
           "to": [
             { "key_code": "return_or_enter" }
                 ]
        }
       ]
     }
  ]
}
